class Program
{
    static void Main()
    {
        FileStream fileStream = null;

        try
        {
            fileStream = new FileStream("example.txt", FileMode.Open);
        }
        catch (Exception ex)
        {
            Console.WriteLine($"Произошло исключение: {ex.Message}");
        }
        finally
        {
            if (fileStream != null)
            {
                fileStream.Close();
            }
        }
    }
}
