using System;

class Program
{
    static void Main(string[] args)
    {
        int[] numbers = { 2,5,7,9,3,1};
        int sum = numbers.ExtArray();
        Console.WriteLine(sum);
    }
} 


public static class Ext
{
    public static int ExtArray(this int[] numsArray)
    {
        int sum = 0;
        for (int i = 0; i < numsArray.Length; i++) 
        {
            sum+= numsArray[i];
        }
        return sum;
    }
}
