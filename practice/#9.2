using System;

class Program
{
    static void Main()
    {
        Console.WriteLine(numAdd(2000, 23));
        Console.WriteLine(numSubtract(2050, 27));
        Console.WriteLine(numMultiply(289, 7));
    }

    static int numAdd(int a, int b)
    {
        return a + b;
    }

    static int numSubtract(int a, int b)
    {
        return a - b;
    }

    static int numMultiply(int a, int b)
    {
        return a * b;
    }
}
