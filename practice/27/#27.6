using System;
using System.Collections;
using System.Collections.Generic;

class MyCollection : IEnumerable<int>
{
    private List<int> data;

    public MyCollection(List<int> inputData)
    {
        data = inputData;
    }

    public IEnumerator<int> GetEnumerator()
    {
        // Используем цикл foreach для итерации по данным
        foreach (var item in data)
        {
            yield return item;
        }
    }

    // Метод, необходимый для реализации интерфейса IEnumerable
    IEnumerator IEnumerable.GetEnumerator()
    {
        return GetEnumerator();
    }
}

class Program
{
    static void Main()
    {
        // Создаем экземпляр класса MyCollection с данными
        List<int> myData = new List<int> { 1, 2, 3, 4, 5 };
        MyCollection myCollection = new MyCollection(myData);

        // Используем цикл foreach для итерации по данным и вывода на экран
        Console.WriteLine("Iterating through the collection:");
        foreach (var item in myCollection)
        {
            Console.WriteLine(item);
        }
    }
}
